@[instance]
protected def is_add_hom.id : ∀ {α : Type u} [_inst_1 : has_add α], is_add_hom id :=
λ {α : Type u} [_inst_1 : has_add α], {map_add := λ (_x _x_1 : α), rfl}
theorem ring_hom.char_p_iff_char_p : ∀ {K : Type u_1} {L : Type u_2} [_inst_1 : field.{u_1} K] [_inst_2 : field.{u_2} L],
  @ring_hom.{u_1 u_2} K L
    (@ring.to_semiring.{u_1} K (@division_ring.to_ring.{u_1} K (@field.to_division_ring.{u_1} K _inst_1)))
    (@ring.to_semiring.{u_2} L (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2))) →
  ∀ (p : nat),
    iff
      (@char_p.{u_1} K
         (@ring.to_semiring.{u_1} K (@division_ring.to_ring.{u_1} K (@field.to_division_ring.{u_1} K _inst_1)))
         p)
      (@char_p.{u_2} L
         (@ring.to_semiring.{u_2} L (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2)))
         p) :=
λ {K : Type u_1} {L : Type u_2} [_inst_1 : field.{u_1} K] [_inst_2 : field.{u_2} L]
(f :
  @ring_hom.{u_1 u_2} K L
    (@ring.to_semiring.{u_1} K (@division_ring.to_ring.{u_1} K (@field.to_division_ring.{u_1} K _inst_1)))
    (@ring.to_semiring.{u_2} L (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2)))) (p : nat),
  @iff.intro
    (@char_p.{u_1} K
       (@ring.to_semiring.{u_1} K (@division_ring.to_ring.{u_1} K (@field.to_division_ring.{u_1} K _inst_1)))
       p)
    (@char_p.{u_2} L
       (@ring.to_semiring.{u_2} L (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2)))
       p)
    (λ
     (_c :
       @char_p.{u_1} K
         (@ring.to_semiring.{u_1} K (@division_ring.to_ring.{u_1} K (@field.to_division_ring.{u_1} K _inst_1)))
         p),
       @char_p.mk.{u_2} L
         (@ring.to_semiring.{u_2} L (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2)))
         p
         (λ (n : nat),
            @eq.mpr.{0}
              (iff
                 (@eq.{u_2+1} L
                    (@coe.{1 u_2+1} nat L
                       (@coe_to_lift.{1 u_2+1} nat L
                          (@nat.cast_coe.{u_2} L
                             (@mul_zero_class.to_has_zero.{u_2} L
                                (@monoid_with_zero.to_mul_zero_class.{u_2} L
                                   (@semiring.to_monoid_with_zero.{u_2} L
                                      (@ring.to_semiring.{u_2} L
                                         (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2))))))
                             (@monoid.to_has_one.{u_2} L
                                (@monoid_with_zero.to_monoid.{u_2} L
                                   (@semiring.to_monoid_with_zero.{u_2} L
                                      (@ring.to_semiring.{u_2} L
                                         (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2))))))
                             (@distrib.to_has_add.{u_2} L
                                (@semiring.to_distrib.{u_2} L
                                   (@ring.to_semiring.{u_2} L
                                      (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2)))))))
                       n)
                    (@has_zero.zero.{u_2} L
                       (@mul_zero_class.to_has_zero.{u_2} L
                          (@monoid_with_zero.to_mul_zero_class.{u_2} L
                             (@semiring.to_monoid_with_zero.{u_2} L
                                (@ring.to_semiring.{u_2} L
                                   (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2))))))))
                 (@has_dvd.dvd.{0} nat nat.has_dvd p n))
              (iff
                 (@eq.{u_2+1} L
                    (@coe.{1 u_2+1} nat L
                       (@coe_to_lift.{1 u_2+1} nat L
                          (@nat.cast_coe.{u_2} L
                             (@mul_zero_class.to_has_zero.{u_2} L
                                (@monoid_with_zero.to_mul_zero_class.{u_2} L
                                   (@semiring.to_monoid_with_zero.{u_2} L
                                      (@ring.to_semiring.{u_2} L
                                         (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2))))))
                             (@monoid.to_has_one.{u_2} L
                                (@monoid_with_zero.to_monoid.{u_2} L
                                   (@semiring.to_monoid_with_zero.{u_2} L
                                      (@ring.to_semiring.{u_2} L
                                         (@division_ring.to_ring.{u_2} L (@field.to_division_ring.{u_2} L _inst_2))))))
